
{
    "name":"Python:FastAPI",
    "module": "uvicorn",
    "type": "python",
    "python": "${env:apiproject}/bin/python",
    "request": "launch",
    "args": [
        "apiproject.main:app",
        "--reload",
        "--port",
        "8000"
    ]
}


